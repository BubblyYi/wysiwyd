
<application>
<name>Dump - Full Robot</name>

        <dependencies>
            <port>/icub/torso/state:o</port>
            <port>/icub/right_arm/state:o</port>
            <port>/icub/left_arm/state:o</port>
            <port>/icub/head/state:o</port>
            <port>/icub/camcalib/left/out</port>
            <port>/icub/camcalib/right/out</port>
        </dependencies>
		
        <module>
            <name>dataDumper </name>
            <parameters>--name dump/icub/torso --type bottle --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperTorso</tag>
        </module>

        <module>
            <name>dataDumper </name>
            <parameters>--name dump/icub/torso --type bottle --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperTorso</tag>
        </module>
		
        <module>
            <name>dataDumper </name>
            <parameters>--name dump/icub/right_arm --type bottle --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperRightArm</tag>
        </module>
		        
		<module>
            <name>dataDumper </name>
            <parameters>--name dump/icub/left_arm --type bottle --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperLeftArm</tag>
        </module>
		
		<module>
            <name>dataDumper </name>
            <parameters>--name dump/icub/head --type bottle --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperHead</tag>
        </module>
		
		<module>
            <name>dataDumper </name>
            <parameters>--name dump/icub/camcalib/left --type image --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperCamCalibLeft</tag>
        </module>
		
		<module>
            <name>dataDumper </name>
            <parameters>--name dump/icub/camcalib/right --type image --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperCamCalibRight</tag>
        </module>
		        
		<module>
            <name>dataDumper </name>
            <parameters>--name dump/OPC --type bottle --rxTime</parameters>
            <node>icub-winnode2</node>
            <tag>dataDumperTorso</tag>
        </module>
		
		<!-- Connections -->
		
		<connection>
			<from>/icub/torso/state:o</from>
			<to>/dump/icub/torso</to>
			<protocol>udp</protocol>
		</connection>
	  
	  	<connection>
			<from>/icub/right_arm/state:o</from>
			<to>/dump/icub/right_arm</to>
			<protocol>udp</protocol>
		</connection>
		  	
		<connection>
			<from>/icub/left_arm/state:o</from>
			<to>/dump/icub/left_arm</to>
			<protocol>udp</protocol>
		</connection>  

		<connection>
			<from>/icub/head/state:o</from>
			<to>/dump/icub/head</to>
			<protocol>udp</protocol>
		</connection>  
		
		<connection>
			<from>/icub/camcalib/left/out</from>
			<to>/dump/icub/camcalib/left</to>
			<protocol>udp</protocol>
		</connection>  
		
		<connection>
			<from>/icub/camcalib/right/out</from>
			<to>/dump/icub/camcalib/right</to>
			<protocol>udp</protocol>
		</connection>
		
		<connection>
			<from>/OPC/broadcast:o</from>
			<to>/dump/OPC</to>
			<protocol>udp</protocol>
		</connection>
		
</application>


