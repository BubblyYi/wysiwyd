<application>
    <name>WYSIWYD DEMO Y2 @INSERM</name>
    <dependencies>
        <port>/icub/camcalib/right/out</port>
        <port>/icub/camcalib/left/out</port>
        <port>/icub/cartesianController/right_arm/state:o</port>
        <port>/icub/cartesianController/left_arm/state:o</port>
        <port>/iKinGazeCtrl/rpc</port>
        <port>/wholeBodyDynamics/rpc:i</port>
    </dependencies>

    <module>
        <name>kinectServer</name>
        <node>ikart</node>
    </module>

    <module>
        <name>SFM</name>
        <parameters></parameters>
        <node>icub-cuda</node>
    </module>
    <module>
        <name>caffeCoder</name>
        <parameters></parameters>
        <node>icub-cuda</node>
    </module>
    <module>
        <name>linearClassifierModule</name>
        <parameters>--BufferSize 1 --CSVM 1.0 --databaseFolder IOL2OPCDatabase
            --WeightedSVM 1</parameters>
        <node>icub-cuda</node>
    </module>

    <module>
        <name>agentDetector</name>
        <parameters>--showImages false</parameters>
        <node>icub-b1</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/kinectServer/rpc</port>
            <port timeout="10">/referenceFrameHandler/rpc</port>
        </dependencies>
    </module>
    <module>
        <name>lbpExtract</name>
        <parameters></parameters>
        <node>icub-b1</node>
    </module>
    <module>
        <name>actionsRenderingEngine</name>
        <parameters>--motor::block_eyes 5.0 --motor::impedance off</parameters>
        <node>icub-b1</node>
    </module>
    <module>
        <name>behaviorManager</name>
        <parameters></parameters>
        <node>icub-b1</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/iSpeak</port>
            <port timeout="10">/speechRecognizer/rpc</port>
            <port timeout="10">/actionsRenderingEngine/cmd:io</port>
            <port timeout="10">/autobiographicalMemory/rpc</port>
        </dependencies>
    </module>
    <module>
        <name>ears</name>
        <parameters></parameters>
        <node>icub-b1</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/BehaviorManager/trigger:i</port>
        </dependencies>
    </module>
    <module>
        <name>proactiveTagging</name>
        <parameters></parameters>
        <node>icub-b1</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/autobiographicalMemory/rpc</port>
            <port timeout="10">/iSpeak</port>
            <port timeout="10">/speechRecognizer/rpc</port>
            <port timeout="10">/lrh/rpc</port>
            <port timeout="10">/actionsRenderingEngine/cmd:io</port>
            <port timeout="10">/babbling/rpc</port>
            <port timeout="10">/agentDetector/rpc</port>
            <port timeout="10">/iol2opc/rpc</port>
        </dependencies>
    </module>
    <module>
        <name>touchDetector</name>
        <parameters></parameters>
        <node>icub-b1</node>
    </module>

    <module>
        <name>objectsPropertiesCollector</name>
        <parameters>--name OPC --no-load-db --no-save-db</parameters>
        <node>icub-b2</node>
    </module>
    <module>
        <name>referenceFrameHandler</name>
        <parameters></parameters>
        <node>icub-b2</node>
    </module>
    <module>
        <name>himrepClassifier</name>
        <parameters></parameters>
        <node>icub-b2</node>
    </module>
    <module>
        <name>guiUpdater</name>
        <parameters></parameters>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
        </dependencies>
        <node>icub-b2</node>
    </module>
    <module>
        <name>homeostasis</name>
        <parameters></parameters>
        <node>icub-b2</node>
    </module>
    <module>
        <name>allostaticController</name>
        <parameters></parameters>
        <node>icub-b2</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/autobiographicalMemory/rpc</port>
        </dependencies>
    </module>
    <module>
        <name>babbling</name>
        <parameters></parameters>
        <node>icub-b2</node>
        <dependencies>
            <port timeout="10">/autobiographicalMemory/rpc</port>
        </dependencies>
    </module>

    <module>
        <name>iolReachingCalibration</name>
        <parameters></parameters>
        <node>icub-b3</node>
    </module>
    <module>
        <name>iol2opc</name>
        <parameters></parameters>
        <node>icub-b3</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/himrepClassifier/rpc</port>
            <port timeout="10">/SFM/rpc</port>
            <port timeout="10">/iolReachingCalibration/rpc</port>
        </dependencies>
    </module>
    <module>
        <name>attentionSelector</name>
        <parameters></parameters>
        <node>icub-b3</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/actionsRenderingEngine/cmd:io</port>
        </dependencies>
    </module>
    <module>
        <name>sensationManager</name>
        <parameters></parameters>
        <node>icub-b3</node>
    </module>
    <module>
        <name>pasar</name>
        <parameters></parameters>
        <node>icub-b3</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/autobiographicalMemory/rpc</port>
            <port timeout="10">/agentDetector/skeleton:o</port>
        </dependencies>
    </module>
    <module>
        <name>narrativeHandler</name>
        <parameters></parameters>
        <node>icub-b3</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
            <port timeout="10">/autobiographicalMemory/rpc</port>
            <port timeout="10">/iSpeak</port>
            <port timeout="10">/speechRecognizer/rpc</port>
            <port timeout="10">/lrh/rpc</port>
        </dependencies>
    </module>

    <module>
        <name>skinManager</name>
        <parameters>--from skinManAll.ini --context skinGui</parameters>
        <node>console1</node>
    </module>
    <module>
        <name>iSpeak</name>
        <parameters></parameters>
        <node>console1</node>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /viewer/iol/lbpSegmented --x 2000 --y 0 --p 50
            --compact</parameters>
        <node>console1</node>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /viewer/iol/localizer --x 2000 --y 380 --out
            /viewer/iol/localizer:o --p 50 --compact</parameters>
        <node>console1</node>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /viewer/iol/histogram --x 2310 --y 380 --w 600 --h
            570 --p 50 --compact</parameters>
        <node>console1</node>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /viewer/iol/track --x 2000 --y 760 --p 50 --compact</parameters>
        <node>console1</node>
    </module>
    <module>
        <name>lrh</name>
        <parameters></parameters>
        <node>console1</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
        </dependencies>
    </module>
    <module>
        <name>autobiographicalMemory</name>
        <parameters></parameters>
        <node>console1</node>
        <dependencies>
            <port timeout="10">/OPC/rpc</port>
        </dependencies>
    </module>

    <module>
        <name>yarpview</name>
        <parameters>--name /viewer/skeleton --compact</parameters>
        <node>display</node>
    </module>
    <module>
        <name>iCubGui</name>
        <parameters>--xpos 1000 --ypos 0</parameters>
        <node>display</node>
    </module>

    <module>
        <name>speechRecognizer</name>
        <parameters></parameters>
        <node>winnode1</node>
    </module>

