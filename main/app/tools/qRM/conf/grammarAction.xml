<GRAMMAR	LANGID="409">

  <DEFINE>
    <ID	NAME="agent"	VAL="1"/>
    <ID	NAME="predicate"	VAL="2"/>
    <ID	NAME="object"	VAL="3"/>
    <ID	NAME="recipient"	VAL="4"/>
  </DEFINE>


  <RULE	NAME="stop"	TOPLEVEL="ACTIVE"	EXPORT="1">
    <P>stop</P>
  </RULE>


  <RULE	NAME="SENTENCE"	TOPLEVEL="ACTIVE"	EXPORT="1">
    <RULEREF	NAME="agent"/>
    <RULEREF	NAME="predicate"/>
    <O>
      <L>
        <P>the</P>
        <P>this</P>
        <P>my</P>
      </L>
    </O>
    <RULEREF	NAME="object"/>
    <O>
      <L>
        <P>to the</P>
        <P>on the</P>
        <P>on</P>
      </L>
    </O>
    <RULEREF	NAME="recipient"/>
  </RULE>


  <RULE NAME="agent" DYNAMIC="TRUE">
    <L>
      <P>I</P>
      <P>you</P>
      <P>icub</P>
      <P>John</P>
      <P>Peter</P>
    </L>
  </RULE>

  <RULE NAME="predicate" DYNAMIC="TRUE">
    <L>
      <P>push</P>
      <P>put</P>
      <P>move</P>
    </L>
  </RULE>

  <RULE NAME="agent" DYNAMIC="TRUE">
    <L>
      <P>I</P>
      <P>you</P>
      <P>icub</P>
      <P>John</P>
      <P>Peter</P>
    </L>
  </RULE>
  
    <RULE NAME="object" DYNAMIC="TRUE">
    <L>
      <P>croco</P>
      <P>guitar</P>
      <P>drums</P>
      <P>keyboard</P>
      <P>mouse</P>
    </L>
  </RULE>

  
    <RULE NAME="recipient" DYNAMIC="TRUE">
    <L>
      <P>quickly</P>
      <P>slowly</P>
      <P>east</P>
      <P>west</P>
      <P>north</P>
      <P>south</P>
      <P>center</P>
      <P>left</P>
      <P>right</P>
      <P>middle</P>
    </L>
  </RULE>


  
</GRAMMAR>
