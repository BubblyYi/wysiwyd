<GRAMMAR  LANGID="409">


  <DEFINE>
    <ID  NAME="object"  VAL="1"/>
    <ID  NAME="adjective"  VAL="2"/>
  </DEFINE>


  <RULE  NAME="SENTENCE1"  TOPLEVEL="ACTIVE"  EXPORT="1">
    <O>
		<P>this is</P>
	<O>
	<O>
		<L>
			<P>the</P>
			<P>an</P>
			<P>a</P>
			<P>my</P>
		</L>
	</O>
	<O>
		<RULEREF  NAME="adjective"/>
	</O>
    <RULEREF  NAME="object"/>
   </RULE>



  <RULE NAME="object" DYNAMIC="TRUE">
    <L>
      <P>cross</P>
      <P>circle</P>
      <P>croco</P>
      <P>cube</P>
      <P>mouse</P>
    </L>
  </RULE>

  <RULE NAME="adjective" DYNAMIC="TRUE">
    <L>
      <P>big</P>
      <P>yellow</P>
    </L>
  </RULE>

</GRAMMAR>