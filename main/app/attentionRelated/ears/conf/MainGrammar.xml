<GRAMMAR	LANGID="809">

  <DEFINE>
    <ID	NAME="predicateObject"	VAL="2"/>
    <ID	NAME="object"	VAL="3"/>
    <ID NAME="questionMark" VAL="6"/>
  </DEFINE>

  <RULE	NAME="stop"	TOPLEVEL="ACTIVE"	EXPORT="1">
    <P>stop</P>
  </RULE>

  <RULE NAME="SENTENCENARRATIVE" TOPLEVEL="ACTIVE" EXPORT="1">
    <RULEREF  NAME="questionMark"/>
    <O>
      <L>
        <P>have you done</P>
        <P>did you just do</P>
        <P>did we do</P>
        <P>did you say</P>
      </L>
    </O>
  </RULE>
  <RULE NAME="questionMark" DYNAMIC="TRUE">
    <L>
      <P>what</P>
    </L>
  </RULE>

  <RULE	NAME="SENTENCEOBJECT"	TOPLEVEL="ACTIVE"	EXPORT="1">
    <RULEREF	NAME="predicateObject"/>
    <O>
      <L>
        <P>the</P>
        <P>this</P>
        <P>my</P>
      </L>
    </O>
    <RULEREF	NAME="object"/>
  </RULE>

  <RULE NAME="predicateObject" DYNAMIC="TRUE">
    <L>
      <P>point</P>
      <P>look at</P>
      <P>push</P>
    </L>
  </RULE>
  
  <RULE NAME="object" DYNAMIC="TRUE">
    <L>
      <P>ladybug</P>
      <P>carrots</P>
      <P>joystick</P>
      <P>mug</P>
      <P>pig</P>
      <P>bottle</P>
    </L>
  </RULE> 
</GRAMMAR>
